{
  "name": "backtalk",
  "version": "0.0.0",
  "private": true,
  "description": "The front end of the FHB lecture evaluation app project.",
  "homepage": "https://github.com/vanruesc/backtalk",
  "main": "src/backtalk",
  "license": "Zlib",

  "keywords": [
		"campus",
		"lecture",
		"evaluation",
		"eval",
		"fhb",
		"university",
		"fachhochschule",
		"brandenburg"
  ],

  "author": {
    "name": "Raoul van Rueschen",
    "email": "vanruesc@fh-brandenburg.de"
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/vanruesc/backtalk.git"
  },

  "bugs": {
    "url": "https://github.com/vanruesc/backtalk/issues"
  },

  "scripts": {
		"jshint:banner": "jshint banner.js",
		"jshint:lib": "jshint src",
		"jshint": "npm run jshint:banner && npm run jshint:lib",
		"prebuild": "npm run jshint",
		"build": "mkdirp build && browserify src/backtalk.js -t babelify -s Backtalk -o build/backtalk.js",
		"uglify": "uglifyjs build/backtalk.js -c -m -o build/backtalk.min.js",
		"pretest": "npm run build",
		"test": "echo \"No tests specified.\"",
		"posttest": "npm run uglify && node banner.js"
  },

  "engines": {
    "node": ">= 0.10.0"
  },

  "dependencies": {
    "overtime": "x.x.x"
  },

  "devDependencies": {
		"babelify": "6.x.x",
		"browserify": "11.x.x",
		"mkdirp": "0.x.x",
		"jshint": "2.x.x",
		"prepend-file": "1.x.x",
		"uglify-js": "2.x.x"
  }

}
